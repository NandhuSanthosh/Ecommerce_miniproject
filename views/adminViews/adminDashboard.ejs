<!DOCTYPE html>
<html>

<head>
    <%- include('../generalPartials/_head.ejs') %>
    <link rel="stylesheet" href="/css/adminDashboard.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/cropperjs/1.5.12/cropper.min.css">

    <title></title>
</head>

<body class="d-flex flex-column">
    <!--Main Navigation-->
    <header>
        <!-- Sidebar -->
        <nav id="sidebarMenu" class="collapse custom-shadow d-lg-block sidebar collapse ">
            <div class="nav-content-container d-flex flex-column h-100">
                <div class="d-flex justify-content-center align-item-center py-3">
                    <a href="">
                        <img width="100px" src="../accets/logo2.png" alt="" style="opacity: .8">
                    </a>
                </div>
                <div class="position-sticky d-flex flex-grow-1 mt-5">
                    <div class="list-group list-group-flush ms-4 mt-5 ps-4" id="dashboard-items">
                        <!-- Elements are rendered using js -->
                    </div>
                </div>
            </div>
        </nav>
        <!-- Sidebar -->

        <div class="highlight-image w-100" height="20px;">
            <img src="../accets/highlight-image.jpg" alt="">
        </div>
        <!-- Navbar --> 
        <nav id="main-navbar" class="navbar  navbar-expand-lg navbar-light ">
            <!-- Container wrapper -->
            <div class="container-fluid">
                <!-- Toggle button -->
                <button class="navbar-toggler" type="button" data-mdb-toggle="collapse" data-mdb-target="#sidebarMenu"
                    aria-controls="sidebarMenu" aria-expanded="false" aria-label="Toggle navigation">
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-list"
                        viewBox="0 0 16 16">
                        <path fill-rule="evenodd"
                            d="M2.5 12a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5zm0-4a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5zm0-4a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5z" />
                    </svg>
                </button>

                <!-- Brand -->
                <a class="navbar-brand" href="#">
                    <img height="45px" src="../accets/logo3.png" alt="">
                </a>
                <!-- Search form -->
                <!-- <form class="d-none d-md-flex input-group w-auto my-auto">
                    <input autocomplete="off" type="search" class="form-control rounded"
                        placeholder='Search (ctrl + "/" to focus)' style="min-width: 225px" />
                    <span class="input-group-text border-0"><i class="fas fa-search"></i></span>
                </form> -->

            </div>
            <!-- Container wrapper -->
        </nav>
        <!-- Navbar -->
    </header>
    <!--Main Navigation-->

    <!--Main layout-->
    <main class="h-100 main-container custom-shadow p-2 me-1 me-md-3 me-lg-5 mb-2" >
        <div id="adminDashboardContents">
            <div id="Dashboard" class="d-none" aria-current="page">
                this is admin dashboard
            </div>
            <div id="Users" class="d-none">
                <%- include("./partials/userSection.ejs") %>

            </div>
            <div id="Orders" class="d-none">this is orders</div>
            <div id="Sellers" class="d-none">this is sellers</div>
            <div id="Products" class="d-none">
                <%- include("./partials/productSection.ejs") %>
            </div>
            <div id="Categories" class="d-none">
                <%- include("./partials/category.ejs") %>
            </div>
        </div>
    </main>
    <!--Main layout-->


    <%- include("../utilityViews/alertModel") %>
        
    <%- include("../generalPartials/_jqueryJs.ejs") %>
    <%- include("../generalPartials/_bootstrapJs.ejs") %>


    <script src="/js/admin/admin_content_render.js"></script>
    <script src="/js/admin/adminCategorySection.js"></script>
    <script src="/js/admin/adminUserSection.js"></script>
    <script src="/js/admin/adminProductSection.js"></script>
    <script src="/js/admin/adminDashboard.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/cropperjs/1.5.12/cropper.min.js"></script>

